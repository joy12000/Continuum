{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "lib": [
      "ES2021",
      "DOM",
      "WebWorker"
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}